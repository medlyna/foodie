<p id="notice"><%= notice %></p>

<h1>Listing Diet Restrictions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diet_restrictions.each do |diet_restriction| %>
      <tr>
        <td><%= link_to 'Show', diet_restriction %></td>
        <td><%= link_to 'Edit', edit_diet_restriction_path(diet_restriction) %></td>
        <td><%= link_to 'Destroy', diet_restriction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diet restriction', new_diet_restriction_path %>
